{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(GDB) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/cpp_09/ex00/btc",
			"args": [
				"${workspaceFolder}/cpp_09/ex00/input.txt"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/cpp_09/ex00/",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "Build with Makefile",
			"logging": {
				"moduleLoad": false,
				"trace": true,
				"engineLogging": false,
				"programOutput": true,
				"debugOutput": true
			},
			"showDisplayString": true,
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"linux": {
				"miDebuggerPath": "/usr/bin/gdb"
			}
		}
	]
}
